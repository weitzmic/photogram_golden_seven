<h1>List of Photos</h1>

<% @list_of_photos.each do |photo| %>

<div>
<img src="<%= photo.source %>"
</div>

<p>
<%= photo.caption %>
</p>

<div>
<a href="http://localhost:3000/photos/<%= photo.id %>">Detail</a>
<a href="/photos/<%= photo.id %>/edit">Edit</a>
<a href="/delete_photo/<%= photo.id %>">Delete</a>
</div>

<% end %>
